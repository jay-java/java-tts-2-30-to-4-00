package Swing;

import javax.swing.JFrame;
import javax.swing.JLabel;

public class DataNotFound {
	JLabel l1;
	DataNotFound(){
		JFrame fr = new JFrame("Alert");
		fr.setSize(300, 200);
		fr.setVisible(true);
		fr.setLayout(null);
	}
	public static void main(String[] args) {
		new DataNotFound();
	}
}
