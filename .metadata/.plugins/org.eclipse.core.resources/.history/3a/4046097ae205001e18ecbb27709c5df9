package basic;
//datatype->
//types 1.Primitive->byte,short,int,long,float,double,boolean,char 
//      2.non-primitive0 -> array,String ,class
public class First {
	public static void main(String[] args) {
		int i = 1;
		int j = i++;
		System.out.println(j);
		
		
		
		
		System.out.println("hi java");
		System.out.println("hi java");
		byte b = 127;
		System.out.println(b);
		float f = 3.148798768765f;
		System.out.println(f);
		double d = 3.1498765645;
		System.out.println(d);
		boolean flag = true;
		System.out.println(flag);
		int i = 1;
		int j = 2;
		int k = i+j;
		
	}
}
