package OOPS;
class Data{
	int id;
	String name;
	static String cname = "TOPS";
	Data(int id, String name){
		this.id=id;
		this.name=name;
	}
	public void showData() {
		System.out.println("id = "+id+"name = "+name+" cname = "+cname);
	}
}
public class StaticKeyword {
	public static void main(String[] args) {
		Data d =new Data();
		d.change();
	}
}
